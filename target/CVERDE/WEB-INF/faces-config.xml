<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>es_PE</supported-locale>
        </locale-config>
        <message-bundle>pe.gob.minam.configuracion.Mensajes</message-bundle>
        <resource-bundle>
            <base-name>pe.gob.minam.configuracion.Mensajes</base-name>
            <var>message</var>	  			  		
        </resource-bundle>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    <managed-bean>
        <managed-bean-name>fileDownloadView</managed-bean-name>
        <managed-bean-class>ape.rgob.lminam.ecommon.scontrolador.FileDownloadView</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>recortarIndicador</managed-bean-name>
        <managed-bean-class>ape.rgob.lminam.ecommon.scontrolador.recortarIndicador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBInstituciones</managed-bean-name>
        <managed-bean-class>pe.gob.minam.sistema.controlador.MBInstituciones</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bKInstituciones</managed-bean-name>
        <managed-bean-class>pe.gob.minam.sistema.controlador.backingBean.BKInstituciones</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBPEvalua</managed-bean-name>
        <managed-bean-class>pe.gob.minam.sistema.controlador.MBPEvalua</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBReportes</managed-bean-name>
        <managed-bean-class>pe.gob.minam.sistema.controlador.MBReportes</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>